{
  "name": "sira-ai-gateway",
  "version": "2.0.0",
  "description": "Sira - 智能API网关，支持多AI服务商智能路由和监控",
  "homepage": "https://github.com/zycxfyh/sira",
  "repository": {
    "type": "git",
    "url": "https://github.com/zycxfyh/sira.git"
  },
  "bugs": {
    "url": "https://github.com/zycxfyh/sira/issues"
  },
  "author": "Student Developer <1666384464@qq.com>",
  "keywords": [
    "ai-gateway",
    "artificial-intelligence",
    "api-gateway",
    "microservices",
    "load-balancing",
    "rate-limiting",
    "circuit-breaker",
    "monitoring",
    "prometheus",
    "grafana",
    "student-project",
    "learning-project"
  ],
  "contributors": [
    "Student Developer <1666384464@qq.com>",
    "Express Gateway Team (Original Framework)"
  ],
  "license": "Apache-2.0",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/zycxfyh"
  },
  "main": "src/index.js",
  "engines": {
    "node": ">= 18.0.0"
  },
  "bin": {
    "eg": "./bin/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "cross-env LOG_LEVEL=debug node src/index.js",
    "start:ai": "cross-env EG_CONFIG_DIR=src/config node src/index.js",
    "setup": "node scripts/setup-config.js",
    "quickstart": "node scripts/setup-config.js --quick && npm start",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "pretest": "echo 'Skipping lint check for now'",
    "test": "cross-env TEST_TYPE=unit jest",
    "test:watch": "cross-env TEST_TYPE=unit jest --watch",
    "test:coverage": "cross-env TEST_TYPE=unit jest --coverage",
    "test:ci": "cross-env TEST_TYPE=unit CI=true jest --ci --coverage --watchAll=false",
    "test:all": "cross-env TEST_TYPE=integration EG_HTTP_PORT=0 EG_CONFIG_DIR=src/config EG_DISABLE_CONFIG_WATCH=true jest --testPathPattern=\"src/test/(?!e2e)\"",
    "test:unit": "cross-env TEST_TYPE=unit jest --testPathPattern=\"src/test/unit\"",
    "test:integration": "cross-env TEST_TYPE=integration jest --testPathPattern=\"src/test/integration\"",
    "test:e2e": "cross-env TEST_TYPE=e2e jest --testPathPattern=\"src/test/e2e\"",
    "test:component": "cross-env TEST_TYPE=unit jest --testPathPattern=\"src/test/component\"",
    "test:ai": "node src/test-ai-gateway.js",
    "test:performance": "cross-env TEST_TYPE=performance jest --testPathPattern=\"src/test/performance\"",
    "test:visual": "cross-env TEST_TYPE=visual jest --testPathPattern=\"src/test/visual\" --passWithNoTests",
    "test:accessibility": "cross-env TEST_TYPE=accessibility jest --testPathPattern=\"src/test/accessibility\" --passWithNoTests",
    "test:scripts": "cross-env TEST_TYPE=unit jest --testPathPattern=\"src/test/scripts\" --passWithNoTests",
    "shellcheck": "find scripts -name \"*.sh\" -type f -exec shellcheck {} \\;",
    "test:integration:setup": "node src/test/mock-ai-server.js & sleep 3 && cross-env EG_CONFIG_DIR=src/config EG_HTTP_PORT=3004 EG_ADMIN_PORT=3005 node src/core/index.js",
    "test:integration:run": "cross-env TEST_TYPE=e2e EG_HTTP_PORT=3002 EG_ADMIN_PORT=3003 jest src/test/e2e --timeout 10000",
    "test:integration:cleanup": "pkill -f 'node src/test/mock-ai-server.js' && pkill -f 'node src/core/index.js'",
    "test:industrial": "node src/run-industrial-tests.js comprehensive",
    "test:industrial:quick": "node src/run-industrial-tests.js quick",
    "test:industrial:performance": "node src/run-industrial-tests.js performance",
    "test:industrial:load": "node src/run-industrial-tests.js load --target-rps 50 --duration 60",
    "test:industrial:stress": "node src/run-industrial-tests.js stress --intensity medium --scenario memory_stress",
    "test:industrial:shell": "./scripts/industrial-testing.sh",
    "test:industrial:ci": "./scripts/industrial-testing.sh --test-type comprehensive --coverage --performance --reports",
    "test:mutation": "stryker run",
    "test:quality": "npm run test:coverage && npm run test:mutation && npm run test:contract",
    "test:quality-gate": "node scripts/utilities/test-quality-gate.js",
    "test:analytics": "node scripts/utilities/test-analytics.js",
    "test:dashboard": "npm run test:analytics && open reports/analytics/dashboard.html",
    "test:benchmark": "node scripts/utilities/performance-benchmark.js",
    "test:benchmark:baseline": "node scripts/utilities/performance-benchmark.js --save-baseline",
    "test:contract": "node scripts/utilities/contract-testing.js",
    "test:contract:ci": "CONTRACT_TEST_BASE_URL=http://localhost:3004 node scripts/utilities/contract-testing.js",
    "test:automation": "node scripts/utilities/test-automation.js",
    "test:generate": "npm run test:automation",
    "test:env": "node scripts/utilities/test-environment-manager.js",
    "test:env:list": "npm run test:env -- list",
    "test:env:switch": "npm run test:env -- switch",
    "test:env:status": "npm run test:env -- status",
    "test:framework": "node src/test/framework/test-framework.js",
    "test:data": "node src/test/data-generator.js",
    "test:coverage:analyze": "node src/test/coverage-analyzer.js",
    "ci:run": "node src/test/ci-runner.js",
    "ci:config": "npm run ci:run -- generate github",
    "admin:start": "node src/admin/index.js",
    "admin:dev": "cross-env NODE_ENV=development node src/admin/index.js",
    "docker:build": "docker build -t sira/gateway:latest .",
    "docker:run": "docker run -p 8080:8080 -p 3001:3001 sira/gateway:latest",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:undeploy": "kubectl delete -f k8s/",
    "deploy:dev": "./scripts/deploy.sh development docker start",
    "deploy:staging": "./scripts/deploy.sh staging docker start",
    "deploy:prod": "./scripts/deploy.sh production docker start",
    "autoscale:cpu": "./scripts/autoscale.sh docker cpu",
    "autoscale:intelligent": "./scripts/autoscale.sh docker intelligent",
    "monitor:system": "./scripts/monitor.sh docker system",
    "monitor:all": "./scripts/monitor.sh docker all",
    "maintenance:daily": "./scripts/maintenance.sh docker daily",
    "maintenance:full": "./scripts/maintenance.sh docker all",
    "performance:optimize": "./scripts/performance-optimizer.sh all",
    "performance:monitor": "./scripts/performance-optimizer.sh monitor",
    "performance:report": "./scripts/performance-optimizer.sh report",
    "alerts:check": "./scripts/alert-manager.sh check-all",
    "alerts:history": "./scripts/alert-manager.sh history 24",
    "test:chaos": "node scripts/utilities/chaos-testing.js",
    "test:chaos:list": "npm run test:chaos -- list",
    "test:chaos:run": "npm run test:chaos -- run",
    "mock:server": "node src/test/mock-ai-server.js",
    "health-check": "node scripts/utilities/health-check.js",
    "format:write": "prettier --write \"**/*.{js,json,md}\"",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "analyze-bundle:ci": "node scripts/analyze-bundle.js --ci --save-report",
    "test:perf": "autocannon -c 100 -d 10 -R 100 http://localhost:8080/health",
    "test:load": "artillery run test/load/load-test.yml",
    "clinic:doctor": "clinic doctor -- node src/core/index.js",
    "clinic:bubbleprof": "clinic bubbleprof -- node src/core/index.js",
    "clinic:flame": "clinic flame -- node src/core/index.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:headers": "curl -I http://localhost:8080/health",
    "cli": "node src/bin/sira-cli.js",
    "cli:start": "npm run cli -- gateway start",
    "cli:stop": "npm run cli -- gateway stop",
    "cli:status": "npm run cli -- gateway status",
    "cli:logs": "npm run cli -- gateway logs",
    "cli:health": "npm run cli -- monitor health",
    "cli:stats": "npm run cli -- monitor stats",
    "generate": "node src/bin/code-generator.js",
    "template": "node src/bin/project-templates.js",
    "deps": "node src/bin/dependency-manager.js",
    "dev-helpers": "node src/bin/dev-helpers.js"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "nyc": {
    "exclude": [
      "logo",
      "node_modules",
      "coverage",
      "migrations",
      "test"
    ]
  },
  "types": "./index.d.ts",
