{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": ["html", "clear-text", "progress", "dashboard"],
  "testRunner": "jest",
  "jest": {
    "projectType": "custom",
    "configFile": "jest.config.js",
    "enableFindRelatedTests": true
  },
  "coverageAnalysis": "perTest",
  "mutate": ["lib/**/*.js", "!lib/config/**/*.json", "!lib/locales/**/*.json"],
  "mutateFilePatterns": [
    "!lib/index.js",
    "!lib/**/*.test.js",
    "!lib/**/*.spec.js"
  ],
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 50
  },
  "timeoutMS": 30000,
  "timeoutFactor": 2,
  "maxConcurrentTestRunners": 4,
  "concurrency": 4,
  "cleanTempDir": true,
  "tempDirName": ".stryker-tmp",
  "buildCommand": "npm run build",
  "commandRunner": {
    "command": "npm test"
  }
}
