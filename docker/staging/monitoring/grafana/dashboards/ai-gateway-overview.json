{
  "dashboard": {
    "id": null,
    "title": "Sira Overview",
    "tags": ["ai-gateway", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Sira Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"ai-gateway\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"ai-gateway\"}[5m])",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"ai-gateway\"}[5m]))",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_gateway_cache_hits_total[5m]) / (rate(ai_gateway_cache_hits_total[5m]) + rate(ai_gateway_cache_misses_total[5m]))",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"ai-gateway\", status=~\"5..\"}[5m]) / rate(http_requests_total{job=\"ai-gateway\"}[5m])",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Container Resources",
        "type": "table",
        "targets": [
          {
            "expr": "container_cpu_usage_seconds_total{container_name=\"ai-gateway\"}",
            "refId": "A"
          },
          {
            "expr": "container_memory_usage_bytes{container_name=\"ai-gateway\"}",
            "refId": "B"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
